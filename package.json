{
  "name": "rump-scriptsify",
  "version": "0.8.0",
  "author": "Juan Placencia",
  "description": "Rump piece for building JS with Browserify",
  "license": "MIT",
  "homepage": "https://github.com/rumps/scriptsify",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/rumps/scriptsify"
  },
  "scripts": {
    "preci": "npm run lint",
    "ci": "babel-node node_modules/.bin/isparta cover --report json _mocha",
    "postci": "codecov < coverage/coverage.json",
    "precompile": "[ -d lib ] && trash lib || true",
    "compile": "babel src --out-dir lib --source-maps=inline",
    "coverage": "babel-node node_modules/.bin/isparta cover --report html --report text _mocha -- -R min",
    "lint": "eslint src test",
    "pub": "npm run test && npm run compile && npm publish",
    "pretest": "npm run lint",
    "test": "mocha"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "browserify": "^10.2.6",
    "bundle-collapser": "^1.2.1",
    "convert-source-map": "^1.1.1",
    "event-stream": "^3.3.1",
    "extend": "^3.0.0",
    "globule": "^0.2.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "through2": "^2.0.0",
    "unreachable-branch-transform": "^0.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-eslint": "^4.0.5",
    "bluebird": "^2.9.34",
    "buffer-equal": "^0.0.1",
    "codecov.io": "^0.1.5",
    "eslint": "^1.0.0",
    "graceful-fs": "^4.1.2",
    "gulp": "^3.9.0",
    "isparta": "^3.0.3",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5",
    "mz": "^2.0.0",
    "rump": "^0.8.0",
    "should": "^7.0.2",
    "should-sinon": "0.0.3",
    "sinon": "^1.15.4",
    "timeout-then": "^1.0.0",
    "trash": "^2.0.0"
  }
}
